<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>getEmployee</servlet-name>
    <servlet-class>com.octans.controller.GetEmployeeController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getEmployee</servlet-name>
    <url-pattern>/getEmployee</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deleteEmployee</servlet-name>
    <servlet-class>com.octans.controller.DeleteEmployeeController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deleteEmployee</servlet-name>
    <url-pattern>/deleteEmployee</url-pattern>
  </servlet-mapping>
</web-app>
